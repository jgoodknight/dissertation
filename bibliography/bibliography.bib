volume = {136},
year = {2012}
}
@article{QPT1,
abstract = {The description of excited state dynamics in energy transfer systems constitutes a theoretical and experimental challenge in modern chemical physics. A spectroscopic protocol that systematically characterizes both coherent and dissipative processes of the probed chromophores is desired. Here, we show that a set of two-color photon-echo experiments performs quantum state tomography (QST) of the one-exciton manifold of a dimer by reconstructing its density matrix in real time. This possibility in turn allows for a complete description of excited state dynamics via quantum process tomography (QPT). Simulations of a noisy QPT experiment for an inhomogeneously broadened ensemble of model excitonic dimers show that the protocol distills rich information about dissipative excitonic dynamics, which appears nontrivially hidden in the signal monitored in single realizations of four-wave mixing experiments.},
author = {Yuen-Zhou, Joel and Krich, Jacob J and Mohseni, Masoud and Aspuru-Guzik, Al{\'{a}}n},
doi = {10.1073/pnas.1110642108},
file = {:Users/joseph/Library/Application Support/Mendeley Desktop/Downloaded/Yuen-Zhou et al. - 2011 - Quantum state and process tomography of energy transfer systems via ultrafast spectroscopy.pdf:pdf},
issn = {1091-6490},
journal = {Proc. Natl. Acad. Sci. U. S. A.},
keywords = {Energy Transfer,Light,Models,Quantum Theory,Theoretical,Tomography,Tomography: methods},
mendeley-groups = {QPT,witness paper},
month = {oct},
number = {43},
pages = {17615--20},
title = {{Quantum state and process tomography of energy transfer systems via ultrafast spectroscopy.}},
url = {http://www.pnas.org/content/108/43/17615.abstract},
volume = {108},
year = {2011}
