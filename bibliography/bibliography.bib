@article{mech4,
abstract = {The prevalence of long-lasting oscillatory signals in two-dimensional (2D) echo spectroscopy of light-harvesting complexes has led to a search for possible mechanisms. We investigate how two causes of oscillatory signals are intertwined: (i) electronic coherences supporting delocalized wavelike motion and (ii) narrow bands in the vibronic spectral density. To disentangle the vibronic and electronic contributions, we introduce a time-windowed Fourier transform of the signal amplitude. We find that 2D spectra can be dominated by excitations of pathways which are absent in excitonic energy transport. This leads to an underestimation of the lifetime of electronic coherences by 2D spectra.},
author = {Kreisbeck, Christoph and Kramer, Tobias and Aspuru-Guzik, Al{\'{a}}n},
doi = {10.1021/jp405421d},
issn = {1520-5207},
journal = {The journal of physical chemistry. B},
